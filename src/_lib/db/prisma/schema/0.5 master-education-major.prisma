model MasterEducationMajor{
    id         String               @id @default(uuid())
    name       String               @db.VarChar(255) @unique

    levels      MasterLevelMajorEducation[]

    education   Education?

    created_at  DateTime @default(now())
    updated_at  DateTime @updatedAt  

    @@map("education_major")
}

model MasterLevelMajorEducation{
    id         String               @id @default(uuid())
    level       MasterEducationLevel?       @relation(fields: [id_level], references: [id])
    major       MasterEducationMajor?       @relation(fields: [id_major], references: [id])
    
    id_level             String      @unique @db.VarChar(36)
    id_major             String      @unique @db.VarChar(36)

    @@index([id_level, id_major])
}