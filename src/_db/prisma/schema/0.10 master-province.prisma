model MasterProvince {
  id                   String                @unique
  name                 String
  id_country           String
  created_at           DateTime              @default(now())
  updated_at           DateTime              @updatedAt
  cities               MasterCity[]
  country              MasterCountry         @relation(fields: [id_country], references: [id])
  personal_information PersonalInformation[]

  @@map("master_province")
}
