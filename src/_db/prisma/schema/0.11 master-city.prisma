model MasterCity {
  id                   String                @unique
  name                 String
  id_province          String
  created_at           DateTime              @default(now())
  updated_at           DateTime              @updatedAt
  province             MasterProvince        @relation(fields: [id_province], references: [id])
  districts            MasterDistrict[]
  personal_information PersonalInformation[]

  @@map("master_city")
}
