model FeatureMenuProject{
    id                  String      @default(uuid()) @unique
    description         String      @db.VarChar(255)
    
    id_menu_project     String      @db.VarChar(36)
    menu_project        MenuProject @relation(fields: [id_menu_project], references: [id])

    created_at          DateTime    @default(now())
    updated_at          DateTime    @updatedAt

    @@map("feature_menu_project")
}