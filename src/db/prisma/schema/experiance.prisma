model Experiance {
    id              String      @id @default(uuid())
    
    id_company      String      @db.VarChar(36)
    company         Company     @relation(fields: [id_company], references: [id])

    id_profession   String      @db.VarChar(36)
    profession      Profession  @relation(fields: [id_profession], references: [id])

    projects        Project[]   

    created_at      DateTime    @default(now())
    updated_at      DateTime    @updatedAt

    @@map("experiance")
}